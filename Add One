#include <stdio.h>
#include <string.h>
int main(void) {
    int t;
    scanf("%d",&t);
    while(t--)
    {
        char n[100000000];
        scanf("%s",n);
        if(n[strlen(n)-1]!='9')
        {
            n[strlen(n)-1]++;
            printf("%s\n",n);
        }
        else {
            int i=strlen(n)-1;
            while(n[i]=='9')
            {
                n[i]='0';
                i--;
            }
           if(i==-1)
           {
               printf("1");
           }
           else
           {
               n[i]++;
           }
            printf("%s\n",n);
            
        }    
    }
	return 0;
}
